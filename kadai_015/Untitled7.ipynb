        "class Human:\n",
        "  def __init__(self,name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def show_info(self):\n",
        "    print(f\"Name/ {self.name}　Age/ {self.age}\")\n",
        "\n",
        "hito = Human(\"samurai\", 20)\n",
        "\n",
        "hito.show_info()"
